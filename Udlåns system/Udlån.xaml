<Window x:Class="Udlåns_system.Udlån"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Udlåns_system"
        mc:Ignorable="d"
        Title="Udlån" Height="230" Width="511" IsEnabled="False">
    <Grid x:Name="grid" Margin="10,10,10,10">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="4*"/>
            <ColumnDefinition Width="0.5*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBlock Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="3">

        </TextBlock>
        <TextBlock Grid.Row="1" Text="Bruger: " FontWeight="Bold" FontSize="18" TextAlignment="Right"/>
        <TextBox Grid.Column="1" Grid.Row="1"/>
        <TextBlock Grid.Row="2" Text="Enhed: " FontWeight="Bold" FontSize="18" TextAlignment="Right"/>
    </Grid>
</Window>
